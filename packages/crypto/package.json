{
  "name": "@uireact/crypto",
  "description": "UiEncrypter for AES encryptions",
  "repository": "https://github.com/inavac182/uireact",
  "license": "GNU",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:publish": "tsup src/index.ts --format cjs,esm --dts",
    "clean": " rm -rf dist node_modules coverage",
    "dev": "tsc",
    "eslint": "eslint ./src/**/*.ts ./__tests__/**/*.ts --max-warnings=0",
    "lint": "npm run tsc && npm run eslint",
    "test": "jest",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "peerDependencies": {
    "tslib": "^2.5.0"
  }
}
